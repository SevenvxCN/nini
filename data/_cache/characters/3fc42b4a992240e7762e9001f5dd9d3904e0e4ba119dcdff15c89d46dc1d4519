 });\\n                const worker = new Worker(URL.createObjectURL(blob));\\n                worker.onmessage = e => {\\n                    STATE.lyrics.current = e.data.original;\\n                    STATE.lyrics.translated = e.data.translated;\\n                    if (STATE.lyrics.current.length === 0) updateLyricsUI('â™ª (çº¯éŸ³ä¹æˆ–æ— æ­Œè¯)');\\n                    else if (!STATE.audio.paused) syncLyrics(STATE.audio.currentTime, true);\\n                };\\n                return worker;\\n            } catch (e) {\\n                console.error(\\\"Worker setup failed:\\\", e);\\n                return null;\\n            }\\n        }\\n\\n        function updateLyricsUI(mainText, transText = '') {\\n            requestAnimationFrame(() => {\\n                const currentMain = DOM.lyricsArea.querySelector('.lyrics-line');\\n                const currentTrans = DOM.lyricsArea.querySelector('.lyrics-translation');\\n\\n                if (currentMain) currentMain.classList.add('exiting');\\n                if (currentTrans) currentTrans.classList.add('exiting');\\n\\n                setTimeout(() => {\\n                    DOM.lyricsArea.innerHTML = ''; // Clear after fade out\\n\\n                    const newMain = document.createElement('div');\\n                    newMain.className = 'lyrics-line';\\n                    newMain.textContent = mainText;\\n                    DOM.lyricsArea.appendChild(newMain);\\n\\n                    if (transText) {\\n                        const newTrans = document.createElement('div');\\n                        newTrans.className = 'lyrics-translation';\\n                        newTrans.textContent = transText;\\n                        DOM.lyricsArea.appendChild(newTrans);\\n                    }\\n\\n                    setTimeout(() => {\\n                       newMain.classList.add('active');\\n                       if (transText) {\\n                           const transEl = DOM.lyricsArea.querySelector('.lyrics-translation');\\n                           if(transEl) transEl.classList.add('active');\\n                       }\\n                    }, 50);\\n\\n                }, 400);\\n            });\\n        }\\n\\n        function findLyricIndex(currentTime) {\\n            if (STATE.lyrics.current.length === 0) return -1;\\n            let i = STATE.lyrics.current.length - 1;\\n            while (i >= 0) {\\n                if (currentTime >= STATE.lyrics.current[i].time) return i;\\n                i--;\\n            }\\n            return -1;\\n        }\\n\\n        function syncLyrics(currentTime, forceUpdate = false) {\\n            const newIndex = findLyricIndex(currentTime);\\n            if (newIndex !== STATE.lyrics.activeIndex || forceUpdate) {\\n                STATE.lyrics.activeIndex = newIndex;\\n                if (newIndex === -1) {\\n                    updateLyricsUI('â™ª');\\n                    return;\\n                }\\n                const mainLyric = STATE.lyrics.current[newIndex].content;\\n                let transLyric = '';\\n                if (STATE.lyrics.showTranslation && STATE.lyrics.translated.length > 0) {\\n                    // Simple time-based matching for translation\\n                    const transMatch = STATE.lyrics.translated.find(t => Math.abs(t.time - STATE.lyrics.current[newIndex].time) < 0.5);\\n                    if(transMatch) transLyric = transMatch.content;\\n                }\\n                updateLyricsUI(mainLyric, transLyric);\\n            }\\n        }\\n\\n        // --- AUDIO & PLAYBACK CONTROL ---\\n        async function playSong(isRetry = false) {\\n            if (musicName === STATE.nowPlaying.name && !isRetry) return;\\n\\n            // å¹¿æ’­\\\"åœæ­¢\\\"å‘½ä»¤ï¼Œè®©å…¶ä»–å®ä¾‹åœæ­¢æ’­æ”¾\\n            audioChannel.postMessage({ command: 'stop_and_replace', id: instanceId });\\n\\n            resetPlayerState();\\n            updateSourceInfo(\\\"æœç´¢ä¸­...\\\");\\n\\n            const result = await WY_MusicGetUrl(musicName, singer) || await QQ_MusicGetUrl(musicName);\\n\\n            if (result && result.url) {\\n                STATE.nowPlaying = { name: musicName, source: result.source, id: result.id };\\n                updateSourceInfo(`æ¥æº: ${result.source}`);\\n                fetchLyrics(result.id, result.source);\\n                STATE.audio.src = result.url;\\n                try {\\n                    await STATE.audio.play();\\n                } catch(e) {\\n                    console.error(\\\"Playback failed:\\\", e);\\n                    updateSourceInfo(\\\"æ’­æ”¾å¤±è´¥, è¯·ç‚¹å‡»\\\");\\n                }\\n            } else {\\n                updateSourceInfo(\\\"æœªæ‰¾åˆ°éŸ³æº\\\");\\n                resetPlayerState();\\n            }\\n        }\\n\\n        function resetPlayerState() {\\n            STATE.audio.pause();\\n            STATE.audio.src = '';\\n            STATE.nowPlaying = { name: null, source: null, id: null };\\n            STATE.lyrics = { current: [], translated: [], showTranslation: STATE.lyrics.showTranslation, activeIndex: -1 };\\n            updateUI(false);\\n            updateLyricsUI('...');\\n            DOM.progressBar.style.width = '0%';\\n        }\\n\\n        function updateUI(isPlaying) {\\n            if(isPlaying){\\n                DOM.playIcon.style.display = 'none';\\n                DOM.pauseIcon.style.display = 'block';\\n            } else {\\n                DOM.playIcon.style.display = 'block';\\n                DOM.pauseIcon.style.display = 'none';\\n            }\\n        }\\n\\n        function updateSourceInfo(text) {\\n            DOM.sourceInfo.textContent = text;\\n            DOM.sourceInfo.style.opacity = '1';\\n            setTimeout(() => { DOM.sourceInfo.style.opacity = '0'; }, 2000);\\n        }\\n\\n        // --- VOLUME CONTROL FUNCTIONS ---\\n        function updateVolumeUI() {\\n            const volume = window.globalVolume;\\n            DOM.volumeSlider.value = Math.round(volume * 100);\\n            \\n            // æ›´æ–°éŸ³é‡æŒ‰é’®å›¾æ ‡\\n            const volumeIcon = DOM.volumeBtn.querySelector('i');\\n            if (volume === 0) {\\n                volumeIcon.className = 'fas fa-volume-mute';\\n            } else if (volume < 0.5) {\\n                volumeIcon.className = 'fas fa-volume-down';\\n            } else {\\n                volumeIcon.className = 'fas fa-volume-up';\\n            }\\n        }\\n\\n        function setGlobalVolume(volume) {\\n            window.globalVolume = volume;\\n            \\n            // å¹¿æ’­éŸ³é‡å˜åŒ–åˆ°æ‰€æœ‰å®ä¾‹\\n            audioChannel.postMessage({ \\n                command: 'volume_change', \\n                volume: volume, \\n                id: instanceId \\n            });\\n            \\n            // æ›´æ–°å½“å‰æ’­æ”¾å™¨éŸ³é‡\\n            if (STATE.audio) {\\n                STATE.audio.volume = volume;\\n            }\\n            \\n            updateVolumeUI();\\n        }\\n\\n        // --- EVENT HANDLERS ---\\n        DOM.playBtn.addEventListener('click', () => {\\n            if (STATE.audio.paused) {\\n                if (STATE.audio.src) {\\n                    // å¹¿æ’­\\\"åœæ­¢\\\"å‘½ä»¤ï¼Œè®©å…¶ä»–å®ä¾‹åœæ­¢æ’­æ”¾\\n                    audioChannel.postMessage({ command: 'stop_and_replace', id: instanceId });\\n                    STATE.audio.play();\\n                } else {\\n                    playSong();\\n                }\\n            } else {\\n                STATE.audio.pause();\\n            }\\n        });\\n\\n        STATE.audio.addEventListener('play', () => updateUI(true));\\n        STATE.audio.addEventListener('pause', () => updateUI(false));\\n        STATE.audio.addEventListener('timeupdate', () => {\\n            if (STATE.audio.duration > 0) {\\n                DOM.progressBar.style.width = `${(STATE.audio.currentTime / STATE.audio.duration) * 100}%`;\\n                syncLyrics(STATE.audio.currentTi