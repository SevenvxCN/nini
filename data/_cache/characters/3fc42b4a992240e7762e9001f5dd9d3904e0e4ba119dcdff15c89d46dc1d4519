¯å¦å¯ç”¨\\n            if (typeof TavernHelper === 'undefined' || typeof getCurrentMessageId === 'undefined' || typeof getChatMessages === 'undefined') {\\n                console.error('APIæ£€æŸ¥å¤±è´¥:', {\\n                    TavernHelper: typeof TavernHelper,\\n                    getCurrentMessageId: typeof getCurrentMessageId,\\n                    getChatMessages: typeof getChatMessages\\n                });\\n                alert('é”™è¯¯ï¼šSillyTavern API æœªæ‰¾åˆ°ã€‚è¯·åœ¨æ­£ç¡®çš„ç¯å¢ƒä¸‹è¿è¡Œã€‚');\\n                return;\\n            }\\n            console.log('APIæ£€æŸ¥é€šè¿‡');\\n\\n            const prompt = `\\n<Request: æ ¹æ®'<è§’è‰²ç”Ÿæˆè®¾å®š>'åŠç”¨æˆ·è¾“å…¥ç”Ÿæˆå¯¹åº”è§’è‰²å¡ï¼Œä¸¥æ ¼æŒ‰ä»¥ä¸‹è¦æ±‚è¾“å‡ºã€‚**æœ¬æ¬¡è¾“å‡ºä¸éœ€è¦è¾“å‡ºgametxtæ­£æ–‡ï¼Œä»…æŒ‰ç…§æœ¬æ¬¡è¾“å…¥è¦æ±‚ä¸­çš„è¾“å‡ºæ ¼å¼å®ä¾‹è¾“å‡ºå³å¯ã€‚>\\n<è§’è‰²ç”Ÿæˆè®¾å®š>\\n# ä»»åŠ¡ï¼šæ ¹æ®ç”¨æˆ·è¾“å…¥çš„è§’è‰²åç§°ï¼Œå‚è€ƒä¸Šä¸‹æ–‡æ•…äº‹ï¼Œæ ¹æ®ä»¥ä¸‹æ­¥éª¤åŠæ¨¡æ¿ï¼Œç”Ÿæˆä¸€ä¸ªå®Œæ•´çš„è§’è‰²è®¾å®š\\n\\n## æ­¥éª¤ï¼š\\n1. **åˆ†æç”¨æˆ·è¾“å…¥**: ç”¨æˆ·æƒ³è¦åˆ›å»ºçš„è§’è‰²åç§°ã€‚\\n   - ç”¨æˆ·è¾“å…¥: ${characterName}\\n\\n2. **å‚ç…§æ¨¡æ¿**: è¯·ä¸¥æ ¼ä½¿ç”¨ä»¥ä¸‹æä¾›çš„è§’è‰²æ¨¡æ¿ä½œä¸ºåŸºç¡€ç»“æ„å’Œé£æ ¼ã€‚å°†æ¨¡æ¿ä¸­çš„æ‰€æœ‰å†…å®¹æ›¿æ¢ä¸ºä¸ç”¨æˆ·è¾“å…¥å’Œè§’è‰²è®¾å®šç›¸ç¬¦çš„å…·ä½“å†…å®¹ï¼ˆä»…å‚è€ƒåŸºç¡€æ ¼å¼å’Œé£æ ¼ï¼ï¼‰ï¼Œç”Ÿæˆè¯¥è§’è‰²çš„äººè®¾ã€‚\\n   - **æ¨¡æ¿**:\\n     ${characterTemplate}\\n   - **å¿…é¡»åŠ å…¥çš„è¡¥å……è®¾å®šæˆ–ä¸å¾—ç”Ÿæˆçš„å†…å®¹**:\\n     2.1 ä¸å¾—æ˜¯éœ¸é“æ€»è£ã€å›½ç‹ã€åˆ†æå¸ˆç­‰éœ¸é“ã€ç†æ€§åˆ†æçš„æ€§æ ¼ï¼Œå°†ä¼šå¯¼è‡´AIåç»­æ‰®æ¼”ææ˜“çŠ¯é”™ï¼›\\n     2.2 **æ³¨æ„ï¼Œåº”ç”Ÿæˆçš„æ˜¯è¯¥è§’è‰²æœ¬èº«çš„äººè®¾ï¼Œå¹¶éè¯¥è§’è‰²åœ¨å‰§æœ¬ä¸­çš„è§’è‰²äººè®¾ï¼›ç¦æ­¢å°†å‰§æœ¬ä¸­çš„äººè®¾å†™å…¥è¿›æ¥ã€‚**\\n\\n3. **ç”Ÿæˆè§’è‰²å¡**: å°†æœ€ç»ˆç”Ÿæˆçš„å®Œæ•´è§’è‰²å¡ï¼Œç”¨ \\\\`<character_card>\\\\` å’Œ \\\\`</character_card>\\\\` æ ‡ç­¾åŒ…è£¹èµ·æ¥ã€‚è¿™æ˜¯å¿…é¡»çš„æ­¥éª¤ï¼Œç”¨äºå†™å…¥ä¸–ç•Œä¹¦ã€‚å¦å¤–åœ¨æ€ç»´é“¾ä¸­ç”¨\\\"character_card\\\"ä»£æŒ‡<character_card>æ ‡ç­¾,é˜²æ­¢å‡ºé”™ã€‚\\n\\n## è¾“å‡ºæ ¼å¼ç¤ºä¾‹:\\n\\n<character_card>\\n[è¿™é‡Œæ˜¯æ ¹æ®æ¨¡æ¿å’Œç”¨æˆ·è¾“å…¥ç”Ÿæˆçš„å®Œæ•´è§’è‰²å¡... ]\\n</character_card>\\n\\n<UpdateVariable>\\n# å˜é‡æ›´æ–°\\n</UpdateVariable>\\n\\n</è§’è‰²ç”Ÿæˆè®¾å®š>\\n            `.trim();\\n\\n            console.log(\\\"æ­£åœ¨å‘AIå‘é€è§’è‰²ç”Ÿæˆè¯·æ±‚...\\\");\\n            const generateConfig = {\\n                injects: [{\\n                    role: 'user',\\n                    content: prompt,\\n                    position: 'before_char',\\n                    should_write_to_chat: false,\\n                    should_scan: true,\\n                }],\\n                should_stream: false,\\n            };\\n            const aiResponse = await TavernHelper.generate(generateConfig);\\n\\n            if (!aiResponse) {\\n                throw new Error(\\\"AIæœªèƒ½ç”Ÿæˆè§’è‰²å†…å®¹ã€‚\\\");\\n            }\\n\\n            // æå–è§’è‰²å¡å†…å®¹\\n            let characterCardContent = null;\\n            const endTag = '</character_card>';\\n            const startTag = '<character_card>';\\n\\n            // ä»æœ«å°¾å¼€å§‹æŸ¥æ‰¾ç»“æŸæ ‡ç­¾\\n            const lastEndTagIndex = aiResponse.lastIndexOf(endTag);\\n            // å¦‚æœæ‰¾åˆ°äº†ç»“æŸæ ‡ç­¾ï¼Œå†ä»å®ƒä¹‹å‰çš„ä½ç½®å¼€å§‹æŸ¥æ‰¾å¼€å§‹æ ‡ç­¾\\n            const lastStartTagIndex = lastEndTagIndex !== -1 ? aiResponse.lastIndexOf(startTag, lastEndTagIndex) : -1;\\n            \\n            // ç¡®ä¿æ‰¾åˆ°äº†æˆå¯¹çš„æ ‡ç­¾\\n            if (lastStartTagIndex !== -1 && lastEndTagIndex !== -1) {\\n                characterCardContent = aiResponse.substring(lastStartTagIndex, lastEndTagIndex + endTag.length).trim();\\n            }\\n\\n            if (characterCardContent) {\\n                console.log(\\\"æˆåŠŸæå–åˆ°è§’è‰²å¡å†…å®¹ï¼Œé•¿åº¦:\\\", characterCardContent.length);\\n                console.log(\\\"è§’è‰²å¡å†…å®¹é¢„è§ˆ:\\\", characterCardContent.substring(0, 200) + \\\"...\\\");\\n                \\n                // åˆ›å»ºä¸–ç•Œä¹¦æ¡ç›®\\n                await createCharacterWorldbook(characterName, characterCardContent);\\n                \\n                // æå–æœ€ç»ˆçš„è§’è‰²åç§°ç”¨äºæç¤º\\n                const extractedName = extractCharacterNameFromContent(characterCardContent);\\n                const finalName = extractedName || characterName;\\n                \\n                alert(`è§’è‰²\\\"${finalName}\\\"å·²æˆåŠŸæ·»åŠ åˆ°ä¸–ç•Œä¹¦ï¼\\\\n\\\\nè¯·æ£€æŸ¥ä¸–ç•Œä¹¦\\\"æ¬²æœ›ä¸–ç•ŒÂ·å®Œå…¨é‡åˆ¶ç‰ˆ\\\"ä¸­çš„æ¡ç›®\\\"ã€äººè®¾-${finalName}ã€‘\\\"`);\\n                closeCharacterModal();\\n            } else {\\n                console.log(\\\"AIå›å¤å†…å®¹:\\\", aiResponse);\\n                alert(\\\"è§’è‰²ç”Ÿæˆå¤±è´¥ï¼šæœªæ‰¾åˆ°å®Œæ•´çš„è§’è‰²å¡æ ‡ç­¾ã€‚\\\\n\\\\nè¯·æ£€æŸ¥AIæ˜¯å¦è¿”å›äº†<character_card>æ ‡ç­¾åŒ…è£¹çš„å†…å®¹ã€‚\\\");\\n            }\\n\\n        } catch (error) {\\n            console.error('ç”Ÿæˆè§’è‰²æ—¶å‡ºé”™:', error);\\n            alert('ç”Ÿæˆè§’è‰²æ—¶å‡ºé”™: ' + error.message);\\n        } finally {\\n            submitBtn.disabled = false;\\n            submitBtn.textContent = originalText;\\n        }\\n    }\\n\\n    // ä»è§’è‰²å¡å†…å®¹ä¸­æå–è§’è‰²åç§°\\n    function extractCharacterNameFromContent(characterContent) {\\n        console.log('å¼€å§‹ä»è§’è‰²å¡å†…å®¹ä¸­æå–è§’è‰²åç§°...');\\n        \\n        // æŸ¥æ‰¾ name: å­—æ®µ\\n        const nameMatch = characterContent.match(/name:\\\\s*([^\\\\n\\\\r]+)/i);\\n        if (nameMatch && nameMatch[1]) {\\n            const extractedName = nameMatch[1].trim();\\n            console.log('æˆåŠŸæå–åˆ°è§’è‰²åç§°:', extractedName);\\n            return extractedName;\\n        }\\n        \\n        // å¦‚æœæ²¡æ‰¾åˆ° name: å­—æ®µï¼Œå°è¯•æŸ¥æ‰¾å…¶ä»–å¯èƒ½çš„æ ¼å¼\\n        const alternativeMatch = characterContent.match(/åç§°[ï¼š:]\\\\s*([^\\\\n\\\\r]+)/i);\\n        if (alternativeMatch && alternativeMatch[1]) {\\n            const extractedName = alternativeMatch[1].trim();\\n            console.log('é€šè¿‡æ›¿ä»£æ ¼å¼æå–åˆ°è§’è‰²åç§°:', extractedName);\\n            return extractedName;\\n        }\\n        \\n        console.warn('æœªèƒ½ä»è§’è‰²å¡å†…å®¹ä¸­æå–åˆ°è§’è‰²åç§°');\\n        return null;\\n    }\\n\\n    // åˆ›å»ºè§’è‰²ä¸–ç•Œä¹¦æ¡ç›®\\n    async function createCharacterWorldbook(characterName, characterContent) {\\n        console.log('createCharacterWorldbook å‡½æ•°è¢«è°ƒç”¨');\\n        console.log('ç”¨æˆ·è¾“å…¥çš„è§’è‰²åç§°:', characterName);\\n        console.log('å†…å®¹é•¿åº¦:', characterContent.length);\\n        \\n        // ä»AIå›å¤ä¸­æå–å®é™…çš„è§’è‰²åç§°\\n        const extractedName = extractCharacterNameFromContent(characterContent);\\n        const finalCharacterName = extractedName || characterName; // å¦‚æœæå–å¤±è´¥ï¼Œä½¿ç”¨ç”¨æˆ·è¾“å…¥çš„åç§°\\n        \\n        console.log('æœ€ç»ˆä½¿ç”¨çš„è§’è‰²åç§°:', finalCharacterName);\\n        \\n        const bookName = 'æ¬²æœ›ä¸–ç•ŒÂ·å®Œå…¨é‡åˆ¶ç‰ˆ';\\n        const entryName = `ã€äººè®¾-${finalCharacterName}ã€‘`;\\n        \\n        console.log('ä¸–ç•Œä¹¦å:', bookName);\\n        console.log('æ¡ç›®å:', entryName);\\n        \\n        try {\\n            console.log('å¼€å§‹æ£€æŸ¥ç°æœ‰ä¸–ç•Œä¹¦æ¡ç›®...');\\n            // æ£€æŸ¥ä¸–ç•Œä¹¦æ˜¯å¦å·²æœ‰åŒåæ¡ç›®\\n            const existingWorldbook = await TavernHelper.getLorebookEntries(bookName);\\n            console.log('è·å–åˆ°çš„ä¸–ç•Œä¹¦æ¡ç›®æ•°é‡:', existingWorldbook.length);\\n            console.log('ç°æœ‰æ¡ç›®åˆ—è¡¨:', existingWorldbook.map(entry => entry.comment));\\n            \\n            const existingEntry = existingWorldbook.find(entry => entry.comment === entryName);\\n            \\n            if (existingEntry) {\\n                console.log('æ‰¾åˆ°åŒåæ¡ç›®ï¼Œè·³è¿‡åˆ›å»º');\\n                alert(`ä¸–ç•Œä¹¦ä¸­å·²å­˜åœ¨åŒåè§’è‰²\\\"${characterName}\\\"ï¼Œè·³è¿‡åˆ›å»ºã€‚`);\\n                return;\\n            }\\n            \\n            console.log('å¼€å§‹åˆ›å»ºæ–°æ¡ç›®...');\\n            \\n            // æ£€æŸ¥æ˜¯å¦æœ‰createWorldbookEntrieså‡½æ•°å¯ç”¨\\n            if (typeof createWorldbookEntries === 'function') {\\n                console.log('ä½¿ç”¨createWorldbookEntriesåˆ›å»ºæ¡ç›®');\\n                const worldbookEntry = {\\n                    name: entryName,\\n                    content: characterContent,\\n                    enabled: true,\\n                    strategy: {\\n                        type: 'selective',\\n                        keys: [finalCharacterName],\\n                        keys_secondary: {\\n                            logic: 'and_any',\\n                            keys: []\\n                        },\\n                        scan_depth: 'same_as_global'\\n                    },\\n                    position: {\\n                        type: 'before_character_definition',\\n                        role: 'system',\\n                        depth: 0,\\n                        order: 100\\n                    },\\n                    recursion: {\\n                        prevent_incoming: true,\\n                        prevent_outgoing: true,\\n                        delay_until: null\\n                    },\\n                    effect: {\\n                        sticky: null,\\n                        cooldown: null,\\n                        delay: null\\n                    }\\n                };\\n                \\n                await createWorldbookEntries(bookName, [worldbookEntry]);\\n                console.log('ä½¿ç”¨createWorldbookEntriesåˆ›å»ºæˆåŠŸ');\\n            } else {\\n                console.log('createWorldbookEntriesä¸å¯ç”¨ï¼Œä½¿ç”¨TavernHelper.setLorebookEntries');\\n                // ä½¿ç”¨TavernHelper.setLorebookEntriesåˆ›å»ºæ–°æ¡ç›®\\n                const result = await TavernHelper.setLorebookEntries(bookName, [{ \\n                    comment: entryName,\\n                    content: characterContent\\n                }]);\\n                console.log('setLorebookEntries è¿”å›ç»“æœ:', result);\\n       