nel = new BroadcastChannel('global_audio_control_channel');\\n        const instanceId = Date.now() + Math.random();\\n        \\n        // å…¨å±€éŸ³é‡å­˜å‚¨\\n        if (!window.globalVolume) {\\n            window.globalVolume = 0.3; // é»˜è®¤30%éŸ³é‡\\n        }\\n        \\n        // ç›‘å¬å…¶ä»–å®ä¾‹çš„æ¶ˆæ¯\\n        audioChannel.onmessage = (event) => {\\n            if (event.data.id !== instanceId) {\\n                if (event.data.command === 'stop_and_replace') {\\n                    // åœæ­¢å½“å‰æ’­æ”¾å™¨\\n                    if (STATE.audio && !STATE.audio.paused) {\\n                        STATE.audio.pause();\\n                        updateUI(false);\\n                    }\\n                } else if (event.data.command === 'volume_change') {\\n                    // åŒæ­¥å…¨å±€éŸ³é‡\\n                    window.globalVolume = event.data.volume;\\n                    updateVolumeUI();\\n                    if (STATE.audio) {\\n                        STATE.audio.volume = window.globalVolume;\\n                    }\\n                }\\n            }\\n        };\\n\\n        // --- DATA & STATE ---\\n        const musicInfoString = `$1`;\\n        const [musicName, singer] = musicInfoString.split(' - ').map(s => s.trim());\\n\\n        const STATE = { audio: new Audio(), lyrics: { current: [], translated: [], showTranslation: false, activeIndex: -1 }, ui: { playbackMode: 'single' }, nowPlaying: { name: null, source: null, id: null } };\\n        const DOM = {\\n            playBtn: document.getElementById('playButton'),\\n            playIcon: document.querySelector('.play-icon'),\\n            pauseIcon: document.querySelector('.pause-icon'),\\n            lyricsArea: document.getElementById('lyricsArea'),\\n            sourceInfo: document.getElementById('sourceInfo'),\\n            progressBar: document.getElementById('progressBar'),\\n            progressContainer: document.getElementById('progressContainer'),\\n            translationTog