  setGlobalVolume(1.0); // ä»é™éŸ³æ¢å¤åˆ°100%\\n            } else {\\n                setGlobalVolume(0); // é™éŸ³\\n            }\\n        });\\n\\n        // --- API & UTILITIES ---\\n        const Http_Get = url => new Promise(r => $.ajax({ url, method: 'GET', timeout: 5000, success:d=>r(d), error:()=>r(null) }));\\n        const checkAudioAvailability = url => new Promise(res => { const t=new Audio();t.src=url; const id=setTimeout(()=>c(false),2000); const c=r=>{clearTimeout(id);res(r);}; t.oncanplaythrough=()=>c(true); t.onerror=()=>c(false); });\\n\\n        async function fetchLyrics(id, source) {\\n            if (!lyricsWorker || !id || !source) return;\\n            const url = source === 'netease' ? `https://api.vkeys.cn/v2/music/netease/lyric?id=${id}` : `https://api.vkeys.cn/v2/music/tencent/lyric?id=${id}`;\\n            const res = await Http_Get(url);\\n            if(res && res.data){\\n                lyricsWorker.postMessage({ lrcText: res.data.lrc || res.data.lyric, transText: res.data.trans || res.data.tlyric });\\n            }\\n        }\\n\\n        async function WY_MusicGetUrl(name, singer) { try { const term = singer ? `${name}-${singer}` : name; const res = await Http_Get(`https://api.vkeys.cn/v2/music/netease?word=${encodeURIComponent(term)}`); if(!res?.data?.length) return null; for(const s of res.data) { if(s.id){ const r = await Http_Get(`https://api.vkeys.cn/v2/music/netease?id=${s.id}`); if(r?.data?.url && await checkAudioAvailability(r.data.url)) return {url:r.data.url, source:\\\"ç½‘æ˜“\\\", id:s.id};}} return null; } catch(e){return null;}}\\n        async function QQ_MusicGetUrl(name) { try { const res = await Http_Get(`https://api.vkeys.cn/v2/music/tencent?word=${encodeURIComponent(name)}`); if(!res?.data?.length) return null; for(const s of res.data.concat(...res.data.map(i=>i.grp||[]))) { if(s.id){ const r = await Http_Get(`https://api.vkeys.cn/v2/music/tencent?id=${s.id}`); if(r?.data?.url && await checkAudioAvailability(