eight;\\n            particles = [];_initParticles();\\n        }\\n        window.addEventListener('resize', resizeCanvas, false);\\n        class Particle {\\n            constructor() {\\n                const container = canvas.parentElement;\\n                this.x = Math.random() * container.offsetWidth;\\n                this.y = Math.random() * container.offsetHeight;\\n                this.vx = (Math.random() - 0.5) * 0.3;\\n                this.vy = (Math.random() - 0.5) * 0.3;\\n                this.radius = Math.random() * 1.2 + 0.5;\\n                this.color = Math.random() > 0.5 ? 'rgba(129, 140, 248, 0.9)' : 'rgba(244, 114, 182, 0.9)';\\n            }\\n            update() { this.x += this.vx; this.y += this.vy; if (this.x < 0 || this.x > canvas.width) this.vx *= -1; if (this.y < 0 || this.y > canvas.height) this.vy *= -1; }\\n            draw() { ctx.beginPath(); ctx.arc(this.x, this.y, this.radius, 0, Math.PI * 2); ctx.fillStyle = this.color; ctx.fill(); }\\n        }\\n        function _initParticles() {\\n            const particleCount = Math.floor(canvas.width * canvas.height / 20000);\\n            for (let i = 0; i < Math.max(20, particleCount); i++) { particles.push(new Particle()); }\\n        }\\n        function animate() { ctx.clearRect(0, 0, canvas.width, canvas.height); particles.forEach(p => { p.update(); p.draw(); }); animationFrameId = requestAnimationFrame(animate); }\\n        resizeCanvas();animate();\\n    })();\\n\\n    function switchTab(event, pageName) {\\n        document.querySelectorAll('.page').forEach(p => p.classList.remove('active'));\\n        document.querySelectorAll('.tab-button').forEach(b => b.classList.remove('active'));\\n        document.getElementById(pageName).classList.add('active');\\n        event.currentTarget.classList.add('active');\\n    }\\n\\n    /**\\n     * å®‰å…¨è·å–å€¼çš„å‡½æ•°ï¼Œæ”¯æŒå„ç§æ•°æ®ç±»å‹\\n     * @param {*} obj - è¦æŸ¥è¯¢çš„å¯¹è±¡\\n     * @param {string} path - è·¯å¾„ï¼Œå¦‚ 'a.b.0'\\n     * @param {*} defaultValue - é»˜è®¤å€¼\\n     * @returns {*} è·å–åˆ°çš„å€¼æˆ–é»˜è®¤å€¼\\n     */\\n    function SafeGetValue(obj, path, defaultValue = \\\"N/A\\\") {\\n        // å¦‚æœè·¯å¾„æ— æ•ˆï¼Œç›´æ¥è¿”å›é»˜è®¤å€¼\\n        if (typeof path !== 'string' || path.trim() === '') return defaultValue;\\n        \\n        // å¦‚æœå¯¹è±¡æœ¬èº«å°±æ˜¯åŸºæœ¬ç±»å‹ï¼Œç›´æ¥è¿”å›\\n        if (obj === null || typeof obj !== 'object') {\\n            return obj !== undefined ? obj : defaultValue;\\n        }\\n        \\n        let keys = path.split('.');\\n        let current = obj;\\n        \\n        for (let i = 0; i < keys.length; i++) {\\n            // å¦‚æœå½“å‰å€¼ä¸æ˜¯å¯¹è±¡/æ•°ç»„ï¼Œå°è¯•è·å–å…¶å±æ€§\\n            if (current === null || current === undefined) {\\n                return defaultValue;\\n            }\\n            \\n            // å¤„ç†æ•°ç»„ç´¢å¼•\\n            const key = keys[i];\\n            if (Array.isArray(current)) {\\n                const index = parseInt(key, 10);\\n                if (!isNaN(index) && index >= 0 && index < current.length) {\\n                    current = current[index];\\n                } else {\\n                    // ç´¢å¼•æ— æ•ˆï¼Œå°è¯•å°†æ•°ç»„è½¬ä¸ºå­—ç¬¦ä¸²\\n                    return current.join(', ') || defaultValue;\\n                }\\n            } \\n            // å¤„ç†å¯¹è±¡\\n            else if (typeof current === 'object' && current.hasOwnProperty(key)) {\\n                current = current[key];\\n            } \\n            // å¤„ç†å…¶ä»–ç±»å‹\\n            else {\\n                // å°è¯•å°†å½“å‰å€¼è½¬ä¸ºå­—ç¬¦ä¸²\\n                return String(current) || defaultValue;\\n            }\\n        }\\n        \\n        // æœ€ç»ˆå€¼å¤„ç†\\n        if (current === undefined || current === null) {\\n            return defaultValue;\\n        }\\n        \\n        // å¤„ç†ç‰¹æ®Šç±»å‹\\n        if (typeof current === 'boolean') {\\n            return current ? 'æ˜¯' : 'å¦';\\n        }\\n        \\n        // å¤„ç†æ•°ç»„\\n        if (Array.isArray(current)) {\\n            return current.length > 0 ? current[0] : defaultValue;\\n        }\\n        \\n        // å¤„ç†å¯¹è±¡\\n        if (typeof current === 'object') {\\n            // å°è¯•è·å–å¯¹è±¡çš„ç¬¬ä¸€ä¸ªå±æ€§å€¼\\n            const values = Object.values(current);\\n            return values.length > 0 ? values[0] : defaultValue;\\n        }\\n        \\n        return current;\\n    }\\n\\n    /**\\n     * æ ¼å¼åŒ–æ–‡æœ¬ï¼Œå¤„ç†å„ç§æ•°æ®ç±»å‹\\n     * @param {*} text - è¦æ ¼å¼åŒ–çš„æ–‡æœ¬\\n     * @returns {string} æ ¼å¼åŒ–åçš„HTMLå­—ç¬¦ä¸²\\n     */\\n    function formatWithSeparators(text) {\\n        // å¤„ç†null/undefined\\n        if (text === null || text === undefined) {\\n            return 'ï¼ˆæš‚æ— æ•°æ®ï¼‰';\\n        }\\n        \\n        // å¤„ç†å¸ƒå°”å€¼\\n        if (typeof text === 'boolean') {\\n            return text ? 'æ˜¯' : 'å¦';\\n        }\\n        \\n        // å¤„ç†æ•°ç»„\\n        if (Array.isArray(text)) {\\n            return text.map(item => String(item).trim()).filter(l => l).join('<br>') || 'ï¼ˆæš‚æ— æ•°æ®ï¼‰';\\n        }\\n        \\n        // å¤„ç†å¯¹è±¡\\n        if (typeof text === 'object') {\\n            try {\\n                // å°è¯•å°†å¯¹è±¡è½¬ä¸ºå­—ç¬¦ä¸²æ•°ç»„\\n                const values = Object.values(text).map(v => String(v));\\n                return values.join('<br>') || 'ï¼ˆå¯¹è±¡æ•°æ®ï¼‰';\\n            } catch (e) {\\n                return 'ï¼ˆå¯¹è±¡æ•°æ®ï¼‰';\\n            }\\n        }\\n        \\n        // å¤„ç†å­—ç¬¦ä¸²\\n        if (typeof text === 'string') {\\n            return text.split('|||').map(l => l.trim()).filter(l => l).join('<br>') || 'ï¼ˆæš‚æ— æ•°æ®ï¼‰';\\n        }\\n        \\n        // å¤„ç†å…¶ä»–åŸºæœ¬ç±»å‹\\n        return String(text);\\n    }\\n\\n    function updateHotnessEffect(hotValueString) {\\n        const container = document.getElementById('hot-value-container');\\n        if (!container) return;\\n\\n        let numericValue = 0;\\n        // ç¡®ä¿å¤„ç†çš„å€¼æ˜¯å­—ç¬¦ä¸²\\n        const s = String(hotValueString || '0').toLowerCase();\\n        const numPart = parseFloat(s.replace(/,/g, ''));\\n        if (!isNaN(numPart)) {\\n            if (s.includes('b') || s.includes('g')) {\\n                numericValue = numPart * 1e9;\\n            } else if (s.includes('m')) {\\n                numericValue = numPart * 1e6;\\n            } else if (s.includes('k') || s.includes('w')) {\\n                numericValue = numPart * (s.includes('w') ? 1e4 : 1e3);\\n            } else {\\n                numericValue = numPart;\\n            }\\n        }\\n\\n        container.classList.remove('hot-level-1', 'hot-level-2', 'hot-level-3', 'hot-level-4');\\n        if (numericValue >= 1e8) container.classList.add('hot-level-4');\\n        else if (numericValue >= 1e7) container.classList.add('hot-level-3');\\n        else if (numericValue >= 1e6) container.classList.add('hot-level-2');\\n        else if (numericValue >= 1e4) container.classList.add('hot-level-1');\\n    }\\n\\n    async function initDisplay() {\\n        try {\\n            const messages = typeof getChatMessages === 'function' ? await getChatMessages(getCurrentMessageId()) : [];\\n            if (!messages || !messages[0] || !messages[0].data || !messages[0].data.stat_data) {\\n                document.getElementById('live-room-name').innerText = \\\"æ— æ³•åŠ è½½çŠ¶æ€æ•°æ®\\\"; return;\\n            }\\n            const allData = messages[0].data.stat_data;\\n\\n            // ç›´æ’­é—´æ•°æ®åŠ è½½\\n            const liveKey = 'ç›´æ’­é—´';\\n            if (allData[liveKey]) {\\n                const live = allData[liveKey];\\n                document.getElementById('live-room-name').innerText = SafeGetValue(live, 'æˆ¿é—´å.0', 'æœªçŸ¥æˆ¿é—´');\\n                const hotValue = SafeGetValue(live, 'çƒ­åº¦.0', '0');\\n                document.getElementById('live-hot-value').innerText = hotValue;\\n                updateHotnessEffect(hotValue);\\n                document.getElementById('live-danmaku-list').innerHTML = formatWithSeparators(SafeGetValue(live, 'å¼¹å¹•åˆ—è¡¨', ''));\\n            } else { document.getElementById('live-room-name').innerText = \\\"ç›´æ’­é—´æ•°æ®ç¼ºå¤±\\\"; }\\n\\n            // çŠ¶æ€æ æ•°æ®åŠ è½½\\n            const statusKey = 'çŠ¶æ€æ ';\\n            if (allData[statusKey]) {\\n                const status = allData[statusKey];\\n                // ä¸ªäººä¿¡æ¯\\n                document.getElementById('status-name').innerText = SafeGetValue(status, 'åç§°', 'æœªçŸ¥è§’è‰²');\\n                const currentScore = SafeGetValue(status, 'å¯ç”¨åˆ†æ•°', 'å¯ç”¨åˆ†æ•°ï¼š0');\\n                document.getElementById('status-score').innerText = currentScore;\\n                const totalScore = SafeGetValue(status, 'ç´¯è®¡åˆ†æ•°', 'ç´¯è®¡åˆ†æ•°ï¼š0');\\n                document.getElementById('status-total-score').innerText = totalScore;\\n                document.getElementById('status-rank').innerText = SafeGetValue(status, 'æ¬²æœ›ä¸–ç•Œæ’å', 'æš‚æ— æ’å');\\n                document.getElementById('status-script').innerText = SafeGetValue(status, 'å½“å‰å‰§æœ¬', 'æœªè¿›å…¥å‰§æœ¬');\\n\\n                // ä»»åŠ¡è®°å½•\\n                const mainTask = SafeGetValue(status, 'ä¸»çº¿ä»»åŠ¡', '');\\n                const sideTask = SafeGetValue(status, 'æ”¯çº¿ä»»åŠ¡', '');\\n                const limitTask = SafeGetValue(status, 'æé™ä»»åŠ¡', '');\\n\\n                // æ¸²æŸ“ä¸»çº¿ä»»åŠ¡\\n                if (String(mainTask).trim() && String(mainTask) !== 'æ— ') {\\n                    document.getElementById('status-main-task').innerHTML = formatWithSeparators(mainTask);\\n                } else {\\n                    document.getElementById('status-main-task').innerHTML = '<span class=\\\"task-empty\\\">æš‚æ— ä¸»çº¿ä»»åŠ¡</span>';\\n                }\\n\\n                // æ¸²æŸ“æ”¯çº¿ä»»åŠ¡\\n                if (String(sideTask).trim() && String(sideTask) !== 'æ— ') {\\n                    document.getElementById('status-side-task').innerHTML = formatWithSeparators(sideTask);\\n                } else {\\n                    document.getElementById('status-side-task').innerHTML = '<span class=\\\"task-empty\\\">æš‚æ— æ”¯çº¿ä»»åŠ¡</span>';\\n                }\\n\\n                // æ¸²æŸ“æé™ä»»åŠ¡\\n                if (String(limitTask).trim() && String(limitTask) !== 'æ— ') {\\n                    document.getElementById('status-limit-task').innerHTML = formatWithSeparators(limitTask);\\n                } else {\\n                    document.getElementById('status-limit-task').innerHTML = '<span class=\\\"task-empty\\\">æš‚æ— æé™ä»»åŠ¡</span>';\\n                }\\n\\n            } else {\\n                // æ•°æ®ç¼ºå¤±æ—¶çš„é»˜è®¤æ˜¾ç¤º\\n                ['status-name', 'status-score', 'status-total-score', 'status-rank', 'status-script'].forEach(id => {\\n                    document.getElementById(id).innerText = \\\"æ•°æ®ç¼ºå¤±\\\";\\n                });\\n                document.getElementById('status-main-task').innerHTML = '<span class=\\\"task-empty\\\">ä»»åŠ¡æ•°æ®ç¼ºå¤±</span>';\\n                document.getElementById('status-side-task').innerHTML = '<span class=\\\"task-empty\\\">ä»»åŠ¡æ•°æ®ç¼ºå¤±</span>';\\n                document.getElementById('status-limit-task').innerHTML = '<span class=\\\"task-empty\\\">ä»»åŠ¡æ•°æ®ç¼ºå¤±</span>';\\n            }\\n        } catch (error) {\\n            console.error(`çŠ¶æ€æ åŠ è½½å‡ºé”™: ${error.message}`, error);\\n            const container = document.querySelector('.status-container');\\n            if (container) container.innerHTML = `<div style='padding: 20px; color: red;'>çŠ¶æ€æ åŠ è½½å‡ºé”™: ${error.message}</div>`;\\n        }\\n    }\\n\\n    // æ¨¡æ‹Ÿæ•°æ®ï¼ˆåŒ…å«å„ç§æ•°æ®ç±»å‹ç”¨äºæµ‹è¯•ï¼‰\\n    if (typeof getChatMessages === 'undefined' || typeof getCurrentMessageId === 'undefined') {\\n        const mockData = {\\n            stat_data: {\\n                \\\"çŠ¶æ€æ \\\": { \\n                    \\\"åç§°\\\": \\\"æµ‹è¯•è§’è‰²\\\", // å­—ç¬¦ä¸²ç±»å‹ï¼ˆåŸæ•°ç»„ï¼‰\\n                    \\\"å¯ç”¨åˆ†æ•°\\\": { value: \\\"å¯ç”¨åˆ†æ•°ï¼š1024\\\" }, // å¯¹è±¡ç±»å‹ï¼ˆåŸæ•°ç»„ï¼‰\\n                    \\\"ç´¯è®¡åˆ†æ•°\\\": [\\\"ç´¯è®¡åˆ†æ•°ï¼š8192\\\"], // åŸå§‹æ•°ç»„ç±»å‹\\n                    \\\"æ¬²æœ›ä¸–ç•Œæ’å\\\": true, // å¸ƒå°”ç±»å‹ï¼ˆåŸæ•°ç»„ï¼‰\\n                    \\\"å½“å‰å‰§æœ¬\\\": [\\\"æ–°æ‰‹æ•™ç¨‹å‰§æœ¬ã€Šå®¶ä¸­çš„æ¬²æœ›ã€‹2äººæœ¬\\\"],\\n                    \\\"ä¸»çº¿ä»»åŠ¡\\\": [\\\"1. æ¢ç´¢å®¢å…æ‰¾åˆ°éšè—çš„æ¬²æœ›æ‰‹å†Œ\\\", \\\"2. ä¸é˜Ÿå‹å®Œæˆè§’è‰²èº«ä»½ç¡®è®¤\\\", \\\"3. ç ´è§£ä¹¦æˆ¿å¯†ç é”è·å–å…³é”®çº¿ç´¢\\\"], // æ•°ç»„ç±»å‹\\n                    \\\"æ”¯çº¿ä»»åŠ¡\\\": \\\"1. å¸®åŠ©NPCä¿®å¤æŸåçš„é€šè®¯è®¾å¤‡|||2. æ”¶é›†3ä¸ªéšè—çš„çºªå¿µå“ï¼ˆå·²æ”¶é›†2/3ï¼‰\\\", // å­—ç¬¦ä¸²ç±»å‹ï¼ˆåŸæ•°ç»„ï¼‰\\n                    \\\"æé™ä»»åŠ¡\\\": { step1: \\\"1. åœ¨10åˆ†é’Ÿå†…é€ƒç¦»èµ·ç«çš„å§å®¤\\\", step2: \\\"2. ä¸ä½¿ç”¨é“å…·å‡»è´¥1åªç²¾è‹±æ€ªç‰©ï¼ˆæœªå®Œæˆï¼‰\\\" } // å¯¹è±¡ç±»å‹ï¼ˆåŸæ•°ç»„ï¼‰\\n                },\\n                \\\"ç›´æ’­é—´\\\": { \\n                    \\\"æˆ¿é—´å\\\": [\\\"[LIVE] å®¶ä¸­çš„æ¬²æœ› - æ–°æ‰‹æ•™ç¨‹\\\"],\\n                    \\\"çƒ­åº¦\\\": 12300, // æ•°å­—ç±»å‹ï¼ˆåŸå­—ç¬¦ä¸²ï¼‰\\n                    \\\"å¼¹å¹•åˆ—è¡¨\\\": [\\\"[21:03:15] ä¸»æ’­å¿«æ‰¾æ‰‹å†Œï¼\\\", \\\"[21:03:20] æ”¯çº¿ä»»åŠ¡çš„çºªå¿µå“åœ¨å¨æˆ¿ï¼\\\", \\\"[21:03:22] æé™ä»»åŠ¡å°å¿ƒæ€ªç‰©ï¼\\\"]\\n                }\\n            }\\n        };\\n        async function getChatMessages(id) { return [{ data: mockData }]; } \\n        function getCurrentMessageId() { return 'mock'; }\\n        console.warn(\\\"SillyTavern environment not detected. Using mock data for display.\\\");\\n    }\\n\\n    /**\\n     * è‡ªåŠ¨å†™å…¥ä¸–ç•Œä¹¦é€»è¾‘ï¼ˆé™é»˜æ‰§è¡Œï¼‰\\n     */\\n    async function autoWriteToLorebook() {\\n        const endTag = '</å‰§æœ¬è¯¦æƒ…>';\\n        const startTag = '<å‰§æœ¬è¯¦æƒ…>';\\n        \\n        // æ£€æŸ¥SillyTavern APIæ˜¯å¦å¯ç”¨\\n        if (typeof TavernHelper === 'undefined' || typeof getCurrentMessageId === 'undefined' || typeof getChatMessages === 'undefined') {\\n            return; // é™é»˜å¤±è´¥ï¼Œä¸æ˜¾ç¤ºä»»ä½•æç¤º\\n        }\\n\\n        try {\\n            const currentMessageId = getCurrentMessageId();\\n            if (currentMessageId === null) {\\n                return; // é™é»˜å¤±è´¥\\n            }\\n            \\n            const messages = await getChatMessages(currentMessageId);\\n            const lastMessage = messages.length > 0 ? messages[messages.length - 1] : null;\\n\\n            if (!lastMessage || !lastMessage.message) {\\n                return; // é™é»˜å¤±è´¥\\n            }\\n\\n            const aiResponse = lastMessage.message;\\n            let characterCardContent = null;\\n            \\n            // ä»æœ«å°¾å¼€å§‹æŸ¥æ‰¾ç»“æŸæ ‡ç­¾\\n            const lastEndTagIndex = aiResponse.lastIndexOf(endTag);\\n            // å¦‚æœæ‰¾åˆ°äº†ç»“æŸæ ‡ç­¾ï¼Œå†ä»å®ƒä¹‹å‰çš„ä½ç½®å¼€å§‹æŸ¥æ‰¾å¼€å§‹æ ‡ç­¾\\n            const lastStartTagIndex = lastEndTagIndex !== -1 ? aiResponse.lastIndexOf(startTag, lastEndTagIndex) : -1;\\n            \\n            // ç¡®ä¿æ‰¾åˆ°äº†æˆå¯¹çš„æ ‡ç­¾ï¼Œå¦‚æœåªæ‰¾åˆ°ä¸€ä¸ªæ ‡ç­¾åˆ™é™é»˜å¤±è´¥\\n            if (lastStartTagIndex !== -1 && lastEndTagIndex !== -1 && lastStartTagIndex < lastEndTagIndex) {\\n                characterCardContent = aiResponse.substring(lastStartTagIndex, lastEndTagIndex + endTag.length).trim();\\n            }\\n\\n            if (characterCardContent) {\\n                const bookName = 'æ¬²æœ›ä¸–ç•ŒÂ·å®Œå…¨é‡åˆ¶ç‰ˆ';\\n                const entryName = 'ã€å½“å‰å‰§æœ¬ã€‘';\\n                \\n                const allEntries = await TavernHelper.getLorebookEntries(bookName);\\n                const targetEntry = allEntries.find(entry => entry.comment === entryName);\\n                \\n                if (targetEntry) {\\n                    await TavernHelper.setLorebookEntries(bookName, [{ uid: targetEntry.uid, content: characterCardContent }]);\\n                    // é™é»˜æˆåŠŸï¼Œä¸æ˜¾ç¤ºä»»ä½•æç¤º\\n                }\\n            }\\n        } catch (error) {\\n            // é™é»˜å¤±è´¥ï¼Œä¸æ˜¾ç¤ºä»»ä½•æç¤º\\n            console.error('Auto write to lorebook failed:', error);\\n        }\\n    }\\n\\n    /**\\n     * ç®¡ç†ä¸–ç•Œä¹¦æ–‡é£æ¡ç›®\\n     * @param {string} style - 'intellectual'ä¸ºæ™ºæ–—æ–‡é£ï¼Œ'pure'ä¸ºçº¯è‰²æ–‡é£\\n     */\\n    async function manageStyleWorldbooks(style) {\\n        try {\\n            if (typeof getCharWorldbookNames !== 'function' || typeof updateWorldbookWith !== 'function') {\\n                console.log('Tavern Helper worldbook functions not available. Skipping worldbook management.');\\n                return;\\n            }\\n\\n            const charBooks = getCharWorldbookNames('current');\\n            const allBookNames = [charBooks.primary, ...charBooks.additional].filter(Boolean);\\n\\n            if (allBookNames.length === 0) {\\n                console.log('æ²¡æœ‰æ‰¾åˆ°ç»‘å®šçš„ä¸–ç•Œä¹¦ï¼Œè·³è¿‡ä¸–ç•Œä¹¦ç®¡ç†ã€‚');\\n                return;\\n            }\\n\\n            const isIntellectual = style === 'intellectual';\\n            console.log(`å¼€å§‹åˆ‡æ¢åˆ°${isIntellectual ? 'æ™ºæ–—' : 'çº¯è‰²'}æ–‡é£...`);\\n\\n            for (const bookName of allBookNames) {\\n                await updateWorldbookWith(bookName, entries => {\\n                    return entries.map(entry => {\\n                        // æ™ºæ–—æ–‡é£ï¼šç¦ç”¨ğŸ’—æ¡ç›®ï¼Œå¯ç”¨ğŸ¥¸æ¡ç›®\\n                        if (isIntellectual) {\\n                            if (entry.name.includes('ğŸ’—')) {\\n                                return { ...entry, enabled: false };\\n                            }\\n                            if (entry.name.includes('ğŸ¥¸')) {\\n                                return { ...entry, enabled: true };\\n                            }\\n                        }\\n                        // çº¯è‰²æ–‡é£ï¼šç¦ç”¨ğŸ¥¸æ¡ç›®ï¼Œå¯ç”¨ğŸ’—æ¡ç›®\\n                        else {\\n                            if (entry.name.includes('ğŸ¥¸')) {\\n                                return { ...entry, enabled: false };\\n                            }\\n                            if (entry.name.includes('ğŸ’—')) {\\n                                return { ...entry, enabled: true };\\n                            }\\n                        }\\n                        // å…¶ä»–æ¡ç›®ä¿æŒä¸å˜\\n                        return entry;\\n                    });\\n                });\\n                console.log(`ä¸–ç•Œä¹¦ '${bookName}' çš„æ–‡é£æ¡ç›®å·²åˆ‡æ¢åˆ°${isIntellectual ? 'æ™ºæ–—' : 'çº¯è‰²'}æ–‡é£ã€‚`);\\n            }\\n\\n            // æ˜¾ç¤ºæ“ä½œç»“æœæç¤º\\n            if (typeof toastr === 'object' && typeof toastr.info === 'function') {\\n                toastr.info(`æ–‡é£å·²åˆ‡æ¢åˆ°${isIntellectual ? 'æ™ºæ–—' : 'çº¯è‰²'}æ–‡é£ï¼šç›¸å…³ä¸–ç•Œä¹¦æ¡ç›®å·²è‡ªåŠ¨è°ƒæ•´ã€‚`, 'æ–‡é£è®¾ç½®');\\n            }\\n        } catch (error) {\\n            console.error('æ›´æ–°ä¸–ç•Œä¹¦æ—¶å‘ç”Ÿé”™è¯¯:', error);\\n        }\\n    }\\n\\n    /**\\n     * ç®¡ç†ä¸–ç•Œä¹¦ç¾åŒ–æ¡ç›®\\n     * @param {boolean} enable - trueä¸ºå¯ç”¨ï¼Œfalseä¸ºç¦ç”¨\\n     */\\n    async function manageBeautifyWorldbooks(enable) {\\n        try {\\n            if (typeof getCharWorldbookNames !== 'function' || typeof updateWorldbookWith !== 'function') {\\n                console.log('Tavern Helper worldbook functions not available. Skipping worldbook management.');\\n                return;\\n            }\\n\\n            const charBooks = getCharWorldbookNames('current');\\n            const allBookNames = [charBooks.primary, ...charBooks.additional].filter(Boolean);\\n\\n            if (allBookNames.length === 0) {\\n                console.log('æ²¡æœ‰æ‰¾åˆ°ç»‘å®šçš„ä¸–ç•Œä¹¦ï¼Œè·³è¿‡ä¸–ç•Œä¹¦ç®¡ç†ã€‚');\\n                return;\\n            }\\n\\n            console.log(`å¼€å§‹${enable ? 'å¯ç”¨' : 'ç¦ç”¨'}ç¾åŒ–ä¸–ç•Œä¹¦æ¡ç›®...`);\\n\\n            for (const bookName of allBookNames) {\\n                await updateWorldbookWith(bookName, entries => {\\n                    return entries.map(entry => {\\n                        // åªå¤„ç†åŒ…å«ğŸ“™ç¬¦å·çš„æ¡ç›®\\n                        if (entry.name.includes('ğŸ“™')) {\\n                            return { ...entry, enabled: enable };\\n                        }\\n                        // å…¶ä»–æ¡ç›®ä¿æŒä¸å˜\\n                        return entry;\\n                    });\\n                });\\n                console.log(`ä¸–ç•Œä¹¦ '${bookName}' çš„ç¾åŒ–æ¡ç›®å·²${enable ? 'å¯ç”¨' : 'ç¦ç”¨'}ã€‚`);\\n            }\\n\\n            // æ˜¾ç¤ºæ“ä½œç»“æœæç¤º\\n            if (typeof toastr === 'object' && typeof toastr.info === 'function') {\\n                let message = `ç¾åŒ–åŠŸèƒ½å·²${enable ? 'å¼€å¯' : 'å…³é—­'}ï¼šç›¸å…³ä¸–ç•Œä¹¦æ¡ç›®å·²è‡ªåŠ¨${enable ? 'å¯ç”¨' : 'ç¦ç”¨'}ã€‚`;\\n                if (!enable) {\\n                    message += ' è¯·ä½¿ç”¨hideæŒ‡ä»¤éšè—å‰æ–‡ï¼Œé¿å…AIå­¦ä¹ æŒç»­ç”Ÿæˆç¾åŒ–';\\n                }\\n                toastr.info(message, 'ç¾åŒ–è®¾ç½®');\\n            }\\n        } catch (error) {\\n            console.error('æ›´æ–°ä¸–ç•Œä¹¦æ—¶å‘ç”Ÿé”™è¯¯:', error);\\n        }\\n    }\\n\\n    document.addEventListener('DOMContentLoaded', () => {\\n        // åˆå§‹åŒ–æ˜¾ç¤ºæ•°æ®\\n        initDisplay();\\n        \\n        // è‡ªåŠ¨æ‰§è¡Œå†™å…¥ä¸–ç•Œä¹¦é€»è¾‘ï¼ˆé™é»˜ï¼‰\\n        autoWriteToLorebook();\\n\\n        // ä¸ºç¾åŒ–æŒ‰é’®æ·»åŠ äº‹ä»¶ç›‘å¬å™¨\\n        document.getElementById('disable-beautify-btn').addEventListener('click', () => {\\n            manageBeautifyWorldbooks(false);\\n        });\\n\\n        document.getElementById('enable-beautify-btn').addEventListener('click', () => {\\n            manageBeautifyWorldbooks(true);\\n        });\\n\\n        // ä¸ºæ–‡é£æŒ‰é’®æ·»åŠ äº‹ä»¶ç›‘å¬å™¨\\n        documen